<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paowang"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/paowang_logo"
        android:label="@string/app_name"
        android:theme="@style/MMTheme.Basic">
        <activity
            android:name="com.paowang.MainActivity"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|locale"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TubatuViewPagerActivity"></activity>
        <activity
            android:name=".MaterialPatternLockViewActivity"></activity>
        <activity
            android:name=".GridPasswordViewActivity"></activity>
    </application>

</manifest>